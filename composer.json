{
    "name": "sierratecnologia/legislateiro",
    "description": "legislateiro",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "legislateiro"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/legislateiro/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/legislateiro/issues",
        "source": "https://github.com/sierratecnologia/legislateiro",
        "docs": "https://github.com/sierratecnologia/legislateiro/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "sierratecnologia/crypto": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Legislateiro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [{"type": "path","url":"../**","options":{"symlink": true}}],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Legislateiro\\LegislateiroProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    },
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "psalm/plugin-laravel": "^1.5",
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "phpmd/phpmd": "^2.13"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
