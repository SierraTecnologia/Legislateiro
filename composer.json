{
    "name": "sierratecnologia/legislateiro",
    "description": "legislateiro",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "legislateiro"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/legislateiro/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/legislateiro/issues",
        "source": "https://github.com/sierratecnologia/legislateiro",
        "docs": "https://github.com/sierratecnologia/legislateiro/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.4",
        "sierratecnologia/crypto": "^0.4.0",
        "sierratecnologia/payment-laravel": "^0.3.0 | ^0.4.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            ".discovery"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": [],
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "sentry/sentry-laravel",
                "laravel/cashier",
                "hyn/multi-tenant",
                "scaffold-interface"
            ]
        }
    }
}
